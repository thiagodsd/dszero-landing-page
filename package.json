  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "firebase": "^10.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "react-scripts": "^3.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "prebuild": "node src/scripts/generatePortfolioItems.js",
    "prestart": "node src/scripts/generatePortfolioItems.js",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "generate:portfolio": "cross-env NODE_OPTIONS=--openssl-legacy-provider node src/scripts/generatePortfolioItems.js",
    "start:dev": "cross-env NODE_OPTIONS=--openssl-legacy-provider npm-run-all --sequential generate:portfolio start"
